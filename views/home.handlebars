<div class="container">
  <h1>Listado de productos</h1>
  <ul>
    {{#each products}}
      <li>
        <h2>{{this.title}}</h2>
        <p>{{this.description}}</p>
        <p class="price">$ {{this.price}}</p>
        <p class="stock">Stock: {{this.stock}}</p>
        <p class="category">Categor√≠a: {{this.category}}</p>
      </li>
    {{/each}}
  </ul>
</div>
