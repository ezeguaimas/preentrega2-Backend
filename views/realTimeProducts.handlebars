<div class="container">
  <h1>Listado de productos Real Time</h1>
  <div id="RTProductsContainer">
    <ul>
      {{#each products}}
        <li>
          <h2>{{this.title}}</h2>
          <p>{{this.description}}</p>
          <p>$ {{this.price}}</p>
          <p> Stock: {{this.stock}}</p>
          <p>Categor√≠a: {{this.category}}</p>
        </li>
      {{/each}}
    </ul>

  </div>
</div>
<script src="/socket.io/socket.io.js"></script>
<script src="/js/index.js"></script>